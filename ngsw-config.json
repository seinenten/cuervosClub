{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css",
          "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css",
          "https://fonts.googleapis.com",
          "https://fonts.gstatic.com",
          "https://fonts.googleapis.com/css2?family=Poppins&amp;display=swap",
          "https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&amp;display=swap",
          "https://fonts.googleapis.com/icon?family=Material+Icons",
          "https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" 
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "cursosPeque√±os-Api",
      "urls": [
        "https://cursos-server.onrender.com/api/cursos?limite=3"
      ],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "24h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "cursos-Api",
      "urls": [
        "https://cursos-server.onrender.com/api/cursos"
      ],
      "cacheConfig": {
        "maxSize": 15,
        "maxAge": "24h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "usuarios-Api",
      "urls": [
        "https://cursos-server.onrender.com/api/usuarios?desde=0"
      ],
      "cacheConfig": {
        "maxSize": 34,
        "maxAge": "24h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    } 
  ]
}
